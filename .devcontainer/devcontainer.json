{
  "name": "Dev-Stack Multi-Repo Workspace",
  "image": "ghcr.io/rcook0/dev-stack:dev",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "remoteUser": "root",
  "remoteEnv": {
    "PATH": "/opt/venv/bin:${containerEnv:PATH}"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-vscode.cpptools",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "postCreateCommand": "/bin/bash .devcontainer/postCreate.sh"
}
